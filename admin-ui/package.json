{
  "name": "mo-admin-template",
  "description": "mo-admin-template",
  "version": "5.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxorlovsky/admin-panel-skeleton"
  },
  "scripts": {
    "clean": "rm -rf ./public/*",
    "lint": "yarn jslint && yarn stylelint",
    "jslint": "node_modules/.bin/eslint ./src/**/*.{js,vue}",
    "stylelint": "node_modules/.bin/stylelint \"./**/*.scss\" --syntax scss",
    "webpack": "node_modules/.bin/webpack --config webpack.config.js",
    "build": "yarn clean && yarn webpack",
    "release": "git pull && yarn && yarn jslint && yarn stylelint && node_modules/.bin/webpack --config webpack.config.js --env.production",
    "watch": "node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@deveodk/vue-tinymce": "1.0.0",
    "axios": "0.19.0",
    "babel-loader": "8.0.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.2",
    "git-commit-msg": "0.3.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "image-loader": "0.0.1",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.12.0",
    "node-sass-glob-importer": "5.3.1",
    "postcss-loader": "3.0.0",
    "pre-commit": "1.2.2",
    "raw-loader": "2.0.0",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.19.0",
    "sass-loader": "7.1.0",
    "stylelint": "10.0.1",
    "tinymce": "4.8.5",
    "uglify-es": "3.3.9",
    "vue": "2.6.10",
    "vue-directive-tooltip": "1.5.1",
    "vue-loader": "15.7.0",
    "vue-notification": "1.3.16",
    "vue-particles": "1.0.9",
    "vue-router": "3.0.6",
    "vue-template-compiler": "2.6.10",
    "vuedraggable": "2.20.0",
    "vuetify": "1.5.13",
    "vuex": "3.1.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dashboard": "3.0.5",
    "webpack-dev-server": "3.3.1"
  },
  "pre-commit": [
    "stylelint",
    "jslint"
  ],
  "git-commit-hook": {
    "types": [
      "feat",
      "fix",
      "chore",
      "docs",
      "refactor",
      "style",
      "perf",
      "test",
      "revert"
    ],
    "lineLength": 72
  }
}
